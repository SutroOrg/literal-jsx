{
  "name": "@literal-jsx/parser",
  "version": "0.1.1",
  "description": "An example parser implementation for Literal JSX using nearley.js + moo",
  "main": "lib/index.js",
  "author": "Kelley van Evert <hello@klve.nl>",
  "homepage": "https://literal-jsx.org/",
  "repository": {
    "type": "git",
    "url": "https://github.com/kelleyvanevert/literal-jsx",
    "directory": "packages/parser"
  },
  "keywords": [
    "literal-jsx",
    "ljsx",
    "jsx",
    "markup",
    "json",
    "nearley",
    "moo"
  ],
  "license": "MIT",
  "dependencies": {
    "deepmerge": "^3.2.0",
    "nearley": "^2.16.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "babel-jest": "^24.8.0",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-preset-minify": "^0.5.0",
    "cross-env": "^5.2.0",
    "jest": "^24.8.0",
    "rimraf": "^2.6.3"
  },
  "scripts": {
    "clean": "rimraf lib",
    "test": "yarn run build:grammar && jest",
    "build:grammar": "npx nearleyc src/grammar.ne -o src/grammar.js",
    "build:js": "cross-env BABEL_ENV=production babel src --out-dir lib",
    "build": "yarn run build:grammar && yarn run build:js",
    "prepare": "yarn run clean && yarn run test && yarn run build"
  }
}
